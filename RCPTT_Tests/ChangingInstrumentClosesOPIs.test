Entry-Name: .content

switch_to_device_screens_view

let [val screen_name "Screen"] {
    add_device_screen
    get-menu "IBEX/Switch Instrument" | click
    with [get-window "Instrument Selector"] {
        get-group Instrument | get-table | select NDW1695
        get-button OK | click
    }
    get-view "Device Screens" | get-button "Edit Device Screens" | click
    with [get-window "Configure Device Screens"] {
        get-group "Device Screens" | get-list | select $screen_name
        get-button Cancel | click
    }
}
