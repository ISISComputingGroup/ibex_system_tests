let [val config01 [test_prefix "config01"]] {
	with [new_config] {
		set_config_description $config01
		save_and_name_config $config01
	}
	
	try -times 50 -delay 200 -command {
		// Open edit config window
		with [edit_config] {
			try -command {
				//Confirm ok button is greyed out to start
				get-button OK | get-property enablement | equals false | verify-true
			    get-list | select_in_list $config01
			    get-button OK | check_button_enabled_and_click
			 } -catch {
			 	cancel_dialog
			 }
		}
	}
//	try -times 5 -delay 500 -command {	  
//	    with [get-window "Confirm Edit Current Configuration"] {
//		    get-button Yes | check_button_enabled_and_click
//	    }
//	}
	//Confirm window opens
	try -times 50 -delay 500 -command {	  
	  get-window "Edit Configuration" | get-label "Editing the current configuration" | get-property "getText()" | equals "Editing the current configuration" | verify-true
	}
}
