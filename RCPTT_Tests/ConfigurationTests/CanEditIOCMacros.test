// Create variables for one config and IOC
let [val config01 [test_prefix "config01"]] [val ioc01 "EUROTHRM_01"] [val macro01 "ADDR_1"] [val value01 "25"] {
	with [new_config] {
		add_ioc $ioc01
		save_and_name_config $config01
	}
	
	wait 1000
	
	with [edit_config $config01] {
		get-tab-folder | get-tab-item IOCs | check_enabled_and_click
		get-table | select $ioc01
    	get-button "Edit IOC" | check_enabled_and_click
    	with [get-window -class IocDialog] {
    		get-table | select "ADDR_1"
    		get-editbox -after [get-label Value] | set-text $value01
    		get-button "Set Macro" | check_enabled_and_click
    		get-button OK | check_enabled_and_click
    	}
    	get-button "Edit IOC" | check_enabled_and_click
    	with [get-window -class IocDialog ] {
    		get-table | get-item $macro01 | get-property "columns[1]" | equals $value01 | verify-true
    		get-button Cancel | check_enabled_and_click
    	}
    	get-button Cancel | check_enabled_and_click
    }
}
