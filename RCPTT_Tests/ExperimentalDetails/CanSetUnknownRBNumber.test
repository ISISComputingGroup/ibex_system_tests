// change rb number using set
let [val new_rb_number 123456] {
    switch_to_details_view    
    set_rb_numbers $new_rb_number
        
    switch_to_dae_view
    
    with [get_dae_run_information] {
    
    	get-group Setup | get-label -after [get-label "RB Number:"] | get-property caption | equals $new_rb_number | verify-true
       	
    }

}

change rb number using enter
let [val new_rb_number 654321] {
    switch_to_details_view
    set_rb_numbers $new_rb_number -use_enter true
        
    switch_to_dae_view
    
    with [get_dae_run_information] {
    
    	get-group Setup | get-label -after [get-label "RB Number:"] | get-property caption | equals $new_rb_number | verify-true
       	
    }

}

