Content-Type: text/ecl
Entry-Name: .content

let [val test_config "rcptt_test_config"] {
	create_empty_config $test_config
	wait 10000
	load_config $test_config
}
wait 10000
try -times 10 -delay 200 -command {
	get-menu "Configuration/Configurations/Delete" | click
	with [get-window "Delete Configurations" | get-list] {
	    get-property "getItems().length" | equals 2 | verify-true
    	get-property "getItems().String[1]" | equals "rcptt_blank" | verify-true
    }
} -finally {
    get-window "Delete Configurations" | get-button Cancel | click
}
