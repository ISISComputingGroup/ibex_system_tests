Entry-Name: .content

// Create a new configuration

get-menu "Configuration/Configurations/New" | click
with [get-window "New configuration"] {
    get-tab-folder | get-tab-item Summary | click
    with [get-group Summary | get-editbox -after [get-label "Description:"]] {
        set-text "startup_config"
    }
    get-button "Save as ..." | click
    with [get-window "Save Configuration As"] {
        with [get-editbox -after [get-label "Name:"]] {
            set-text startup
        }
        get-button OK | click
    }
}

// Load the new configuration

get-menu "Configuration/Configurations/Load" | click
with [get-window "Load Configuration"] {
    get-list | select startup
    get-button OK | click
}

// Can take a while for the instrument to update, wait 3 mins (sorry!)
try -times 100 -delay 1800 -command {
	assert_instrument_is_setup
}

