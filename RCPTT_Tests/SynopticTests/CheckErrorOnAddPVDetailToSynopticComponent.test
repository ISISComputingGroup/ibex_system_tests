// Create variables for one configuration, one synoptic and one components with a pv details with an error in
let [val synoptic01 [test_prefix "synoptic01"]] [val beamstop "BEAMSTOP"] [val pv_details_name "new pv"]
 {

	with [new_synoptic] {
		// Add eight two components
		add_component $beamstop
		
		add_pv_to_synoptic_component $beamstop $pv_details_name ""
		
		try -times 5 -delay 200 -command {
		    get-editbox -after [get-label Description -after [get-label Value]] -index 1 | get-property text 
		        | contains [concat "In " $beamstop ", " $pv_details_name ": PV Address invalid, must not be empty"] | verify-true
		}
	}
	
	
	
}

